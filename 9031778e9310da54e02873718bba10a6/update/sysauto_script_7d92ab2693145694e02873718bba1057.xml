        <script><![CDATA[(function () {
	
	var sessionGr = new GlideRecord('sys_user_session');
	sessionGr.addNotNullQuery('invalidated');
	sessionGr.orderByDesc('invalidated');
	sessionGr.query();

	var sessions = GlideSessions.get();
	while (sessionGr.next()) {
		var session = sessions.getSession(sessionGr.id);
		if (session) {
			session = session.getHS();
		}

		if (session) {
			var attachments = httpSession.getAttribute('att_guest_acls_attachments');
			if (attachments) {
				gs.info(sessionGr.id + ': ' + attachments);
			}
		}
	}

})();]]></script>
